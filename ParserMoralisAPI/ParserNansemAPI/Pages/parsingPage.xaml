<Page x:Class="ParserNansemAPI.Pages.parsingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ParserNansemAPI.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="1000"
      Title="parsingPage">
    <Grid>
        <StackPanel>
            <TextBlock Name="tbxStatus" Text="Status:" Style="{StaticResource Header}"/>
            <WrapPanel>
                <TextBlock Text="Address:" Style="{StaticResource Header}"/>
                <TextBox x:Name="tbxAddress" Style="{StaticResource Address}"/>
            </WrapPanel>
            <Button x:Name="btnParsing" Content="Parsing"/>
        </StackPanel>
    </Grid>
</Page>
